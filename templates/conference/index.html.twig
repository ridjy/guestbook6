{% extends 'base.html.twig' %}

{% block title %}{{ 'Conference Guestbook'|trans }}{% endblock %}

{% block body %}
    <h2>{{ 'Give your feedback!'|trans }}</h2>

    {% for conference in conferences %}
        <h4>{{ conference }}</h4>
        <p>
            <a href="{{ path('conference', { slug: conference.slug }) }}">{{ 'View'|trans }}</a>
        </p>
    {% endfor %}
{% endblock %}
